{
	"name": "SpatialSink",
	"properties": {
		"linkedServiceName": {
			"referenceName": "lgndatalake",
			"type": "LinkedServiceReference"
		},
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(utcnow(),'updates.csv')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('US/extracts/spatial/in/', formatDateTime(trigger().startTime, 'yyyy'), '/', formatDateTime(trigger().startTime, 'MM'), '/', formatDateTime(trigger().startTime, 'dd'))",
					"type": "Expression"
				},
				"container": "raw"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}